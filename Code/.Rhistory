n <- 1000
sig <- 2
x1 <- rnorm(n,2,1)
x2 <- rnorm(n,-4,4)
y <- rep(0,n)
for (i in 1:n){
y[i] <- rnorm(1,x1[i]+x2[i],sig)
}
f1 <- lm(y~x1+x2)
f2 <- lm(y~x1)
summary(f1)
summary(f2)
